page{width:100vw;height:100vh;font-family:PingFang SC;background:#eee}._div.img-box{position:relative;width:100%;height:100vh}image{max-width:100%;position:absolute;top:0;left:0;width:100%;height:100%}._div.wrapper{width:100%;height:100vh;background:-webkit-linear-gradient(transparent,transparent),-webkit-linear-gradient(left,hsla(0,0%,100%,.2) 1rpx,transparent 0,transparent 19rpx),-webkit-linear-gradient(hsla(0,0%,100%,.2) 1rpx,transparent 0,transparent 19rpx),-webkit-linear-gradient(transparent,#d0837e);background:linear-gradient(transparent,transparent),linear-gradient(90deg,hsla(0,0%,100%,.2) 1rpx,transparent 0,transparent 19rpx),linear-gradient(hsla(0,0%,100%,.2) 1rpx,transparent 0,transparent 19rpx),linear-gradient(transparent,#d0837e);background-size:100% 1.5%,10% 100%,100% 8%,100% 100%;background-repeat:no-repeat,repeat,repeat,no-repeat;background-position:0 0,0 0,0 0,0 0;-webkit-clip-path:polygon(0 0,100% 0,100% 1.5%,0 1.5%);clip-path:polygon(0 0,100% 0,100% 1.5%,0 1.5%);-webkit-animation:move 2s infinite linear;animation:move 2s infinite linear}@-webkit-keyframes move{to{background-position:0 100%,0 0,0 0,0 0;-webkit-clip-path:polygon(0 0,100% 0,100% 100%,0 100%);clip-path:polygon(0 0,100% 0,100% 100%,0 100%)}}@keyframes move{to{background-position:0 100%,0 0,0 0,0 0;-webkit-clip-path:polygon(0 0,100% 0,100% 100%,0 100%);clip-path:polygon(0 0,100% 0,100% 100%,0 100%)}}.bottom{position:fixed;width:100%;height:100rpx;background:#fff;position:fixed;bottom:0;left:0;z-index:100}.bottom .btn{margin:10rpx auto;width:400rpx;height:80rpx;line-height:80rpx;font-size:30rpx;color:#fff;background-image:-webkit-gradient(linear,left top,right top,from(#e50304),color-stop(80%,#f74300));background-image:-webkit-linear-gradient(left,#e50304,#f74300 80%);background-image:linear-gradient(left,#e50304,#f74300 80%);text-align:center;-webkit-border-radius:20rpx;border-radius:20rpx}