page{-webkit-box-sizing:border-box;box-sizing:border-box;background:#eee}textarea{padding:30rpx;width:700rpx;height:500rpx;margin:25rpx;-webkit-border-radius:20rpx;border-radius:20rpx;-webkit-box-sizing:border-box;box-sizing:border-box;border:1rpx solid #dedede;font-size:32rpx;background:#fff}.num{display:block;text-align:right;color:#999;padding:10rpx 30rpx}.btn{margin:50rpx auto;width:400rpx;height:80rpx;line-height:80rpx;font-size:30rpx;color:#fff;background-image:-webkit-gradient(linear,left top,right top,from(#e50304),color-stop(80%,#f74300));background-image:-webkit-linear-gradient(left,#e50304,#f74300 80%);background-image:linear-gradient(left,#e50304,#f74300 80%)}.picInfo{margin:25rpx 25rpx 50rpx;background:#fff;border:1rpx solid #e6e6e6;padding:0 20rpx 30rpx;-webkit-box-sizing:border-box;box-sizing:border-box;width:700rpx;-webkit-border-radius:20rpx;border-radius:20rpx}.picInfo .advice{font-size:32rpx;color:#e2e2e2;margin:30rpx 0 0 0}.picInfo .picView{width:143rpx;height:143rpx;position:relative;margin:30rpx 20rpx 0 0;display:inline-block}.picInfo .picView .pics{width:100%;height:100%;vertical-align:middle}.picInfo .uploadPics{width:143rpx;height:143rpx;vertical-align:middle;margin:30rpx 20rpx 0 0}.picInfo .delete{width:29rpx;height:29rpx;position:absolute;top:-10rpx;right:-10rpx;z-index:999}