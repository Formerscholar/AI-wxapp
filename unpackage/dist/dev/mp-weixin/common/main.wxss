.fmt-emphasis-words, .fmt-cycle-words {
  position: relative;
  text-decoration: none;
}
.fmt-emphasis-words::before, .fmt-cycle-words::before {
  position: absolute;
  content: '';
  width: 100%;
  height: 4rpx;
  bottom: -4rpx;
  left: 0;
  background: url(data:image/png;base64,) repeat-x left center;
}
.fmt-cycle-words::before {
  bottom: 0;
  top: -4rpx;
}
.fmt-wavy-words {
  position: relative;
  text-decoration: none;
}
.fmt-wavy-words::before {
  position: absolute;
  content: '';
  width: 100%;
  height: 4rpx;
  bottom: -4rpx;
  left: 0;
  background: url(data:image/png;base64,) repeat-x 0 center;
}
.fmt-du-words {
  position: relative;
  text-decoration: none;
}
.fmt-du-words::before {
  position: absolute;
  content: '';
  width: 100%;
  height: 7rpx;
  bottom: -7rpx;
  left: 0;
  background: url(data:image/png;base64,) repeat-x 0 center;
}
