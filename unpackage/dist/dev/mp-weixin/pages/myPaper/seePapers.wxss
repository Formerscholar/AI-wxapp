page {
  width: 100vw;
  height: 100vh;
  background: #eee;
  font-family: PingFang SC;
}
.subtitle {
  height: 70rpx;
  line-height: 70rpx;
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: center;
  background: #fff;
  color: #2c2c2c;
  font-size: 32rpx;
  position: fixed;
  z-index: 999;
  top: 0;
  left: 0;
  border-bottom: 1rpx solid #cccccc;
}
.subtitle text {
  color: #e50304;
}
.item:nth-of-type(1) {
  margin-top: 95rpx;
}
.item {
  padding: 30rpx 30rpx 0;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  width: 700rpx;
  margin: 25rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  background: #fff;
  color: #999;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  border: 1rpx solid #f9f9f9;
}
.item .bottom {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 28rpx;
  color: #666666;
  border-top: 1rpx solid #f9f9f9;
  padding: 15rpx 0 15rpx;
  margin-top: 30rpx;
}
.item .bottom .select image {
  width: 35rpx;
  height: 35rpx;
  vertical-align: middle;
  margin: 0 15rpx 0 0;
}
.item .bottom .btn {
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
  padding: 10rpx 30rpx;
}
.item .bottom .btn image {
  width: 35rpx;
  height: 35rpx;
  vertical-align: middle;
  margin: 0 15rpx 0 0;
}
.item:nth-last-of-type(1) {
  margin-bottom: 50rpx;
}
.leixing {
  z-index: 999;
  width: 650rpx;
  background: #fff;
  padding: 30rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
}
.leixing .title {
  text-align: center;
  color: #e50304;
  font-size: 34rpx;
  padding: 0 0 30rpx 0;
  border-bottom: 1rpx solid #e6e6e6;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-bottom: 30rpx;
}
.leixing .title .sameType {
  color: #4c4c4c;
  font-weight: bold;
}
.leixing .title image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.leixing scroll-view {
  width: 100%;
  height: 500rpx;
}
.leixing scroll-view .list {
  z-index: 999;
  overflow: hidden;
}
.leixing scroll-view .list button {
  float: right;
  margin: 20rpx;
  color: #F8601C;
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
  padding: 0;
  background: #fff;
  width: 159rpx;
  font-size: 20rpx;
}
.leixing scroll-view .btnCon {
  text-align: center;
  color: #666666;
  font-size: 30rpx;
  padding: 30rpx 0 30rpx 0;
  border-bottom: 1rpx solid #e6e6e6;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-bottom: 30rpx;
}
.leixing scroll-view .btnCon image {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.leixing scroll-view .list:nth-last-of-type(1) .btnCon {
  border-bottom: none;
}
.leixing .button {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  border-top: 1rpx solid #e6e6e6;
}
.leixing .button button {
  margin: 30rpx 30rpx 0 0;
  font-size: 24rpx;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  background-image: -webkit-gradient(linear, left top, right top, from(#e50304), color-stop(80%, #f74300));
  background-image: -webkit-linear-gradient(left, #e50304 0%, #f74300 80%);
  background-image: linear-gradient(left, #e50304 0%, #f74300 80%);
}
.leixing .button button:nth-of-type(1) {
  border: 1rpx solid #e7e7e7;
  color: #b4b4b4;
  background: none;
}
.kong {
  margin-top: 0;
}
.kong image {
  margin: 225rpx auto 50rpx;
  width: 196rpx;
  height: 237rpx;
}
.kong view {
  text-align: center;
  font-size: 30rpx;
  line-height: 50rpx;
  color: #c4c5c6;
}

