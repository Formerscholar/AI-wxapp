page {
  width: 100vw;
  height: 100vh;
  background: #eee;
  font-family: PingFang SC;
}
.list.stu {
  padding-top: 150rpx;
  /* .l-item:nth-of-type(1){
		margin-top:175rpx;
	} */
}
.list.stu .tabar {
  height: 150rpx;
  padding: 30rpx;
  border-bottom: 1rpx solid #e7e7e7;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 200;
}
.c-c {
  color: #e50304;
}
.list .l-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border: 1rpx solid #e6e6e6;
  padding: 30rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  margin: 25rpx;
  background: #fff;
}
.list .l-item .num {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  color: #a5a5a5;
  font-size: 26rpx;
}
.list .l-item .num image {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: auto 0;
  vertical-align: middle;
  width: 50rpx;
  height: 50rpx;
}
.list .l-item .num > view {
  margin-left: 30rpx;
}
.list .l-item .num > view .paperTitle {
  color: #303030;
  font-size: 32rpx;
  margin: 0 0 15rpx 0;
  width: 520rpx;
}
.list .l-item .right {
  width: 14rpx;
  height: 26rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: auto 0;
}
.list .l-item .download {
  width: 40rpx;
  height: 50rpx;
}
.kong {
  margin-top: 0;
}
.kong image {
  margin: 225rpx auto 50rpx;
  width: 196rpx;
  height: 237rpx;
}
.kong view {
  text-align: center;
  font-size: 30rpx;
  line-height: 50rpx;
  color: #c4c5c6;
}
.mask2 {
  z-index: 999;
  width: 500rpx;
  height: 300rpx;
  background: #fff;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  padding-bottom: 30rpx;
}
.mask2 view:first-child {
  font-size: 32rpx;
  color: #000;
  width: 100%;
  text-align: center;
  margin-top: 80rpx;
  padding-top: 30rpx;
  position: relative;
}
.mask2 view:first-child image {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 35rpx;
  left: 105rpx;
}
.mask2 .put {
  margin-top: 30rpx;
  height: 60rpx;
  width: 430rpx;
  margin: 50rpx auto;
  border: 1rpx solid #e50304;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  position: relative;
}
.mask2 .put input {
  height: 100%;
  width: 80%;
  margin: 0 0 0 75rpx;
  font-size: 24rpx;
  color: #c0c0c0;
  text-align: left;
}
.mask2 .put image {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 15rpx;
  left: 30rpx;
}
.mask2 .btnCon {
  font-size: 0;
}
.mask2 button:nth-of-type(1) {
  background: transparent;
  border: 1rpx solid #c0c0c0;
  color: #c0c0c0;
}
.mask2 button {
  color: #FFFFFF;
  display: inline-block;
  width: 200rpx;
  height: 60rpx;
  font-size: 32rpx;
  line-height: 60rpx;
  background-image: -webkit-gradient(linear, left top, right top, from(#e50304), color-stop(80%, #f74300));
  background-image: -webkit-linear-gradient(left, #e50304 0%, #f74300 80%);
  background-image: linear-gradient(left, #e50304 0%, #f74300 80%);
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  margin-left: 35rpx;
}

