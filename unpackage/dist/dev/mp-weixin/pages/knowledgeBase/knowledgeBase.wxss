.leixing {
  z-index: 999;
  width: 650rpx;
  background: #fff;
  padding: 30rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
}
.leixing .title {
  text-align: center;
  color: #e50304;
  font-size: 34rpx;
  padding: 0 0 30rpx 0;
  border-bottom: 1rpx solid #e6e6e6;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-bottom: 30rpx;
}
.leixing .title .sameType {
  color: #4c4c4c;
  font-weight: bold;
}
.leixing .title image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.leixing scroll-view {
  width: 100%;
  height: 500rpx;
}
.leixing scroll-view .list {
  z-index: 999;
  overflow: hidden;
}
.leixing scroll-view .list button {
  float: right;
  margin: 20rpx;
  color: #F8601C;
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
  padding: 0;
  background: #fff;
  width: 159rpx;
  font-size: 20rpx;
}
.leixing scroll-view .btnCon {
  text-align: center;
  color: #666666;
  font-size: 30rpx;
  padding: 30rpx 0 30rpx 0;
  border-bottom: 1rpx solid #e6e6e6;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-bottom: 30rpx;
}
.leixing scroll-view .btnCon image {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.leixing scroll-view .list:nth-last-of-type(1) .btnCon {
  border-bottom: none;
}
.leixing .button {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  border-top: 1rpx solid #e6e6e6;
}
.leixing .button button {
  margin: 30rpx 30rpx 0 0;
  font-size: 24rpx;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  background-image: -webkit-gradient(linear, left top, right top, from(#e50304), color-stop(80%, #f74300));
  background-image: -webkit-linear-gradient(left, #e50304 0%, #f74300 80%);
  background-image: linear-gradient(left, #e50304 0%, #f74300 80%);
}
.leixing .button button:nth-of-type(1) {
  border: 1rpx solid #e7e7e7;
  color: #b4b4b4;
  background: none;
}
.zt {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: #666;
  opacity: 0.5;
  z-index: 99;
}
.pickes {
  height: 70rpx;
  line-height: 70rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  width: 100%;
  position: fixed;
  background: #fff;
  border-bottom: 1rpx solid #e7e7e7;
  z-index: 99;
}
.pickes picker {
  width: 25%;
}
.pickes .value {
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  position: relative;
  color: #4c4c4c;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.pickes .value image {
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  width: 13rpx;
  height: 11rpx;
  margin: 0 0 0 8rpx;
}

