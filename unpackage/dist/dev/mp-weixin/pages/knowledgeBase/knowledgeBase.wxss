  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  background: #fff;
  color: #999;
  margin: 25rpx;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  border: 1rpx solid #f9f9f9;
}
.item .bottom {
