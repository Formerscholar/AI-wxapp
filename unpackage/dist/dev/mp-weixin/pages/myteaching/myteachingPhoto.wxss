page {
  width: 100vw;
  font-family: PingFang SC;
  background: #eee;
}
.name {
  height: 28rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  margin: 24rpx;
}
.teachingCon {
  border: 1rpx solid #e7e7e7;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  margin: 0 24rpx;
  background: #fff;
}
.teachingCon .card:nth-of-type(1) {
  -webkit-border-radius: 20rpx 20rpx 0 0;
          border-radius: 20rpx 20rpx 0 0;
}
.teachingCon .card:nth-last-of-type(1) {
  -webkit-border-radius: 0 0 20rpx 20rpx;
          border-radius: 0 0 20rpx 20rpx;
}
.listAttr {
  margin: 0 25rpx;
}
.listAttr .card {
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  border: 1rpx solid #e7e7e7;
}
.card {
  height: 180rpx;
  padding: 30rpx 0;
  margin: 0 30rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: relative;
  background: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: 1px solid #e5e5e5;
}
.card:last-child {
  border-bottom: none;
}
.card image.subject {
  width: 99rpx;
  height: 120rpx;
  margin: 0 30rpx 0 0;
  vertical-align: middle;
}
.card .title {
  font-size: 0;
}
.card .title view {
  color: #9a9a9a;
  font-size: 20rpx;
}
.card .title view:nth-of-type(1) {
  color: #666666;
  font-size: 30rpx;
  width: 430rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.card .title .term {
  margin: 2.5% 0;
}
.card .title .version {
  font-size: 20rpx;
  color: #e50304;
  border: 1rpx solid #e50304;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  text-align: center;
  padding: 2rpx 15rpx;
  display: inline-block;
}
.card image.detail {
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  top: 50%;
  right: 35rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.nomore {
  margin: 25rpx 30rpx;
  background: #fff;
  border: 1rpx solid #e7e7e7;
  height: 100rpx;
  line-height: 100rpx;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  text-align: center;
  color: #999;
  font-size: 24rpx;
}
.pickes {
  height: 86rpx;
  margin: 0 24rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 30rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  background: #fff;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  border: 1rpx solid #e7e7e7;
}
.pickes .value {
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  width: 200rpx;
  height: 100rpx;
  line-height: 100rpx;
  position: relative;
  color: #808080;
  font-size: 28rpx;
  font-weight: 500;
  text-align: center;
}
.pickes .value image {
  position: absolute;
  top: 50%;
  right: 10rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  width: 12rpx;
  height: 22rpx;
}
.photo {
  width: 232rpx;
  height: 232rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: 26rpx auto;
}
.tabar {
  height: 142rpx;
  padding: 30rpx;
  border-bottom: 1rpx solid #e7e7e7;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.c-c {
  color: #e50304;
}

