.analysisbox {
  background: #000;
}
.analysisbox .img-box {
  position: relative;
  width: 100%;
  height: 100vh;
}
.analysisbox .img-box .res_img {
  max-width: 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.analysisbox .img-box .wrapper {
  width: 100%;
  height: 100vh;
  background: -webkit-linear-gradient(transparent, transparent), -webkit-linear-gradient(left, #ffffff33 1rpx, transparent 0, transparent 19rpx), -webkit-linear-gradient(#ffffff33 1rpx, transparent 0, transparent 19rpx), -webkit-linear-gradient(transparent, #d0837e);
  background: linear-gradient(transparent, transparent), linear-gradient(90deg, #ffffff33 1rpx, transparent 0, transparent 19rpx), linear-gradient(#ffffff33 1rpx, transparent 0, transparent 19rpx), linear-gradient(transparent, #d0837e);
  background-size: 100% 1.5%, 10% 100%, 100% 8%, 100% 100%;
  background-repeat: no-repeat, repeat, repeat, no-repeat;
  background-position: 0% 0%, 0 0, 0 0, 0 0;
  -webkit-clip-path: polygon(0% 0%, 100% 0%, 100% 1.5%, 0% 1.5%);
          clip-path: polygon(0% 0%, 100% 0%, 100% 1.5%, 0% 1.5%);
  -webkit-animation: move 2s infinite linear;
          animation: move 2s infinite linear;
}
.analysisbox .img-box .bottom {
  position: fixed;
  width: 100%;
  height: 100rpx;
  background: #000;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 100;
}
.analysisbox .img-box .bottom .btn {
  margin: 10rpx auto;
  width: 400rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #fff;
  background-image: -webkit-linear-gradient(left, #e50304 0%, #f74300 80%);
  background-image: linear-gradient(left, #e50304 0%, #f74300 80%);
  text-align: center;
  border-radius: 20rpx;
}
@-webkit-keyframes move {
to {
    background-position: 0 100%, 0 0, 0 0, 0 0;
    -webkit-clip-path: polygon(0% 0%, 100% 0%, 100% 100%, 0% 100%);
            clip-path: polygon(0% 0%, 100% 0%, 100% 100%, 0% 100%);
}
}
@keyframes move {
to {
    background-position: 0 100%, 0 0, 0 0, 0 0;
    -webkit-clip-path: polygon(0% 0%, 100% 0%, 100% 100%, 0% 100%);
            clip-path: polygon(0% 0%, 100% 0%, 100% 100%, 0% 100%);
}
}
