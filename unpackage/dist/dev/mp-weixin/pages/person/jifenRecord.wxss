page {
  background: #eee;
  font-family: PingFang SC;
}
.top {
  width: 100%;
  height: 167rpx;
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  background: url(//aictb.oss-cn-shanghai.aliyuncs.com/wx_xcx/bg/recordBg.png) no-repeat top left;
  background-size: cover;
}
.top view:nth-of-type(1) {
  font-size: 48rpx;
  padding: 20rpx 0;
}
.subtitle {
  font-size: 36rpx;
  padding: 0 30rpx;
  height: 90rpx;
  line-height: 90rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  color: #4e4d4d;
  border-bottom: 1rpx solid #e6e6e6;
  background: #fff;
}
.jifenCon .item {
  margin: 25rpx;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  border: 1rpx solid #e6e6e6;
  background: #fff;
  padding: 30rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  color: #303030;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 26rpx;
  position: relative;
}
.jifenCon .item .paperTitle {
  font-size: 32rpx;
  margin-bottom: 20rpx;
}
.jifenCon .item .time {
  color: #b9b9b9;
}
.jifenCon .item .jifen {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: auto 0;
}
.kong {
  margin-top: 0;
}
.kong image {
  margin: 230rpx auto 50rpx;
  width: 196rpx;
  height: 237rpx;
}
.kong view {
  text-align: center;
  font-size: 30rpx;
  line-height: 50rpx;
  color: #c4c5c6;
}

