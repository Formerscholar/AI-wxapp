<view class="changePass"><image class="bg2" src="https://aictb.oss-cn-shanghai.aliyuncs.com/App/registe_bg.png" mode="widthFix"></image><view class="registe"><view class="zhanhao"><input type="text" placeholder="请输入新手机号" data-event-opts="{{[['input',[['__set_model',['','new_mobile','$event',[]]]]]]}}" value="{{new_mobile}}" bindinput="__e"/><image hidden="{{!(new_mobile.length>0)}}" src="https://aictb.oss-cn-shanghai.aliyuncs.com/App/input_del.png" mode="widthFix" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"></image></view><view class="setcode"><input class="code" type="text" placeholder="验证码" data-event-opts="{{[['input',[['__set_model',['','code2','$event',[]]]]]]}}" value="{{code2}}" bindinput="__e"/><text data-event-opts="{{[['tap',[['getcode2',['$event']]]]]}}" class="getCode" bindtap="__e">{{getText}}</text></view><button class="loginClick" type="default" data-event-opts="{{[['tap',[['baocun',['$event']]]]]}}" bindtap="__e">修改</button></view></view>